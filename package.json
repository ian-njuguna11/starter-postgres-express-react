{
  "name": "starter-postgres-express-react",
  "version": "0.6.4",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.2.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.5",
    "babel-preset-airbnb": "^2.6.0",
    "babel-runtime": "6.26.0",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "1.1.3",
    "concurrently": "^4.1.0",
    "css-loader": "2.1.1",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "extract-text-webpack-plugin": "3.0.2",
    "faker": "^4.1.0",
    "file-loader": "1.1.11",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "http-errors": "^1.7.2",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.13",
    "massive": "^5.10.0",
    "morgan": "^1.9.1",
    "node-pg-migrate": "^3.21.1",
    "nodemon": "^1.19.1",
    "object-assign": "4.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "promise": "8.0.3",
    "prop-types": "^15.7.2",
    "raf": "3.4.1",
    "react": "^16.8.6",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "resolve": "1.11.0",
    "style-loader": "0.23.1",
    "supertest": "^3.4.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "0.6.2",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.5",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "start": "npm run server:prod",
    "build": "node scripts/build.js && npm run lint",
    "lint": "eslint 'src-client/**/*.{js,jsx}' 'src-server/**/*.js'",
    "seed": "npm run db:seed",
    "client:dev": "node scripts/start-client-dev.js",
    "server:dev": "nodemon --watch src-server scripts/start-server-dev.js",
    "server:prod": "node scripts/start-server-prod.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "pg-migrate": "node-pg-migrate",
    "db:migrate": "node scripts/db-migrate",
    "db:seed": "npm run db:migrate && node scripts/db-seed.js",
    "test:client": "node scripts/test.js --env=jsdom",
    "test:server": "node scripts/test.js --config=config/jest/server.json",
    "update-version": "npm --no-git-tag-version version patch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run update-version && git add package.json package-lock.json"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src-client/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src-client/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src-client/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "airbnb"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "comma-dangle": "off",
      "no-console": "off",
      "global-require": "off",
      "jsx-a11y/label-has-for": [
        "error",
        {
          "required": "some"
        }
      ],
      "jsx-a11y/label-has-associated-control": "off",
      "react/prefer-stateless-function": "warn",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:5000/",
      "ws": false
    },
    "/auth": {
      "target": "http://localhost:5000/",
      "ws": false
    }
  },
  "devDependencies": {
    "husky": "^3.0.2"
  }
}
